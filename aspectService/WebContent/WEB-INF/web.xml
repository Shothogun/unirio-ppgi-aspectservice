<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>aspectService</display-name>
  <description>
        The YAWL Aspect Service weave advices to process mdoels
    </description>
  <context-param>
    <param-name>InterfaceX_Service</param-name>
    <param-value>org.yawlfoundation.yawl.aspectService.AspectService</param-value>
  </context-param>
  <context-param>
    <param-name>InterfaceX_BackEnd</param-name>
    <param-value>http://localhost:8080/yawl/ix</param-value>
  </context-param>
  <context-param>
    <param-name>InterfaceBWebSideController</param-name>
    <param-value>org.yawlfoundation.yawl.aspectService.AspectService</param-value>
  </context-param>
  <context-param>
    <param-name>InterfaceB_BackEnd</param-name>
    <param-value>http://localhost:8080/yawl/ib</param-value>
  </context-param>
  <context-param>
    <param-name>EngineLogonUserName</param-name>
    <param-value>aspectService</param-value>
  </context-param>
  <context-param>
    <param-name>EngineLogonPassword</param-name>
    <param-value>yAspect</param-value>
  </context-param>
  <filter>
    <filter-name>CharsetFilter</filter-name>
    <filter-class>org.yawlfoundation.yawl.util.CharsetFilter</filter-class>
    <init-param>
      <param-name>requestEncoding</param-name>
      <param-value>UTF-8</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>CharsetFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>InterfaceX_Servlet</servlet-name>
    <servlet-class>
              org.yawlfoundation.yawl.engine.interfce.interfaceX.InterfaceX_ServiceSideServer
          </servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>InterfaceX_Servlet</servlet-name>
    <url-pattern>/ix</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>aspectService</servlet-name>
    <servlet-class>
            org.yawlfoundation.yawl.engine.interfce.interfaceB.InterfaceB_EnvironmentBasedServer
        </servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>aspectService</servlet-name>
    <url-pattern>/ib</url-pattern>
  </servlet-mapping>
</web-app>